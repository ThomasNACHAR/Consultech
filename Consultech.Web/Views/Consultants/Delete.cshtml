@using Consultech.Web.Models.Consultants
@model ConsultantViewModel

@{
    ViewData["Title"] = "Supprimer un Consultant";
}

<h1>Supprimer</h1>

<h3>Êtes vous sûr de vouloir supprimer ce Consulter</h3>
<div>
    <h4>Consultant</h4>
    <hr />

    <partial name="_PartialDetail" model="@Model" />

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index">Retour à la liste</a>
    </form>
</div>